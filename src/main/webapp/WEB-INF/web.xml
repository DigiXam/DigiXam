<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.IMAP.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegistrationController</servlet-name>
  	<display-name>RegistrationController</display-name>
  	<description></description>
  	<servlet-class>com.IMAP.controller.RegistrationController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OTPVerification</servlet-name>
  	<display-name>OTPVerification</display-name>
  	<description></description>
  	<servlet-class>com.IMAP.controller.OTPVerification</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ForgotPasswordController</servlet-name>
  	<display-name>ForgotPasswordController</display-name>
  	<description></description>
  	<servlet-class>com.IMAP.controller.ForgotPasswordController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NewPasswordController</servlet-name>
  	<display-name>NewPasswordController</display-name>
  	<description></description>
  	<servlet-class>com.IMAP.controller.NewPasswordController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistrationController</servlet-name>
  	<url-pattern>/RegistrationController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OTPVerification</servlet-name>
  	<url-pattern>/OTPVerification</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ForgotPasswordController</servlet-name>
  	<url-pattern>/ForgotPasswordController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NewPasswordController</servlet-name>
  	<url-pattern>/NewPasswordController</url-pattern>
  </servlet-mapping>
  
  <!-- Remove Extensions -->
  <!--login.jsp -->
  <servlet>
  	<servlet-name>login</servlet-name>
  	<jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <!--home.jsp -->
  <servlet>
  	<servlet-name>home</servlet-name>
  	<jsp-file>/home.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <!-- End -->
  <!--forgotpassword.jsp -->
  <servlet>
  	<servlet-name>forgotpassword</servlet-name>
  	<jsp-file>/forgotpassword.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>forgotpassword</servlet-name>
  	<url-pattern>/forgotpassword</url-pattern>
  </servlet-mapping>
  <!-- End -->
  <!--otp.jsp -->
  <servlet>
  	<servlet-name>otp</servlet-name>
  	<jsp-file>/otp.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>otp</servlet-name>
  	<url-pattern>/otp</url-pattern>
  </servlet-mapping>
  <!-- End -->
  <!--newpassword.jsp -->
  <servlet>
  	<servlet-name>newpassword</servlet-name>
  	<jsp-file>/newpassword.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>newpassword</servlet-name>
  	<url-pattern>/newpassword</url-pattern>
  </servlet-mapping>
  <!-- End -->
</web-app>
